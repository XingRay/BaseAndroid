<resources>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/touming</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="window_from_bottom">
        <item name="android:windowEnterAnimation">@anim/animation_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/animation_bottom_out</item>
    </style>
</resources>
